  "jest": {
    "collectCoverageFrom": ["js/*.{js,jsx}", "js/**/*.{js,jsx}"],
    "moduleDirectories": ["<rootDir>/node_modules", "<rootDir>/js"],
    "setupFiles": ["jest-localstorage-mock"],
    "setupTestFrameworkScriptFile": "<rootDir>/js/setupTests.js",
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/js/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/js/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testPathIgnorePatterns": ["<rootDir>/node_modules/"],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      "^(?!.*\\.(js|jsx|json)$)": "<rootDir>/fileTransform.js"
    },
    "transformIgnorePatterns": ["[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"]
  },
  "lint-staged": {
    "**/*.{js,jsx,json}": [
      "prettier --print-width 80 --single-quote --trailing-comma es5 --write",
      "git add"
    ]
  },
  "scripts": {
    "compile": "NODE_ENV=production webpack -p",
    "eslint-check":
      "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "precommit": "lint-staged",
    "start": "webpack --watch-stdin --progress --color",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  }
