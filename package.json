    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.4.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/web/static/js/**/*.{js,jsx}"
    ],
    "moduleDirectories": [
      "<rootDir>/node_modules",
      "<rootDir>/web/static/js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/web/static/js/setupTests.js",
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/web/static/js/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/web/static/js/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      "^.+\\.(css|scss)$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|scss|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  },
  "lint-staged": {
    "web/static/**/*.{js,jsx,json,css}": [
      "prettier --print-width 80 --single-quote --trailing-comma es5 --write",
      "git add"
    ]
  },
  "scripts": {
    "compile": "NODE_ENV=production webpack -p",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "precommit": "lint-staged",
    "start": "webpack --watch-stdin --progress --color",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  }
}
